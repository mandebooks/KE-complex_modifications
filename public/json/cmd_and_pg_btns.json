{
  "title": "Add Command to page_down and page_up",
  "rules": [
    {
      "description": "Add Command to page_down",
      "manipulators": [
        {
          "from": {
            "key_code": "page_down"
          },
          "to": [
            {
              "key_code": "page_down",
              "modifiers": [
                "command"
              ]
            }
          ],
          "type": "basic"
        }
      ],
      "description": "Add Command to page_up",
      "manipulators": [
        {
          "from": {
            "key_code": "page_up"
          },
          "to": [
            {
              "key_code": "page_up",
              "modifiers": [
                "command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}